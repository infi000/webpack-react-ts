{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "semi": [
            "warn",
            "always"
        ],
        "quotes": [
            "warn",
            "double"
        ]
    }
}